<% for comment in comments %>
  <tr>
    <td><%=h comment.commenter %></td>
    <td><%=h comment.body %></td>
    <td><%= link_to 'Show', article_comment_path(article, comment) %></td>
    <td><%= link_to 'Edit', edit_article_comment_path(article, comment) %></td>
    <td><%= link_to 'Destroy', article_comment_path(article, comment), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>
